  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@svgr/cli": "5.5.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^22.10.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-input-mask": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^13.0.0",
    "eslint-define-config": "^2.1.0",
